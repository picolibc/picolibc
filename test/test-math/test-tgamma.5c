real
my_gamma(real x)
{
	if (x <= 0 && x == floor(x))
		raise nan();
	return gamma(x);
}

void
main()
{
	for (int i = 0; i < dim(test_values); i++)
		compute_real_one(test_values[i], my_gamma);
	for (real r = 1e-38; r < 40; r *= 3) {
		real ri = imprecise(r, prec);
		compute_real_one(ri, my_gamma);
		compute_real_one(-ri, my_gamma);
	}
}

main();
